<html>
  <head>
    <title>W02: Task 1</title>
  </head>
  <body>
    <script>
      //var a,b,c;
      var Vec3 = function(a,b,c){
      this.a=a;
      this.b=b;
      this.c=c;
      }
      Vec3.prototype.min = function(){
        if(this.a<=this.b&&this.a<=this.c){
	  return this.a;
 	}else if(this.b<=this.a&&this.b<=this.c){
	    return this.b;
           }else return this.c;
        };
       Vec3.prototype.max = function(){
        if(this.a>=this.b&&this.a>=this.c){
	  return this.a;
 	}else if(this.b>=this.a&&this.b>=this.c){
	     return this.b;
           }else return this.c;
        };
	Vec3.prototype.mid = function(){
        if((this.a<=this.b&&this.a>=this.c)||(this.a>=this.b&&this.a<=this.c)){
	  return this.a;
 	}else if((this.b<=this.a&&this.b>=this.c)||(this.b>=this.a&&this.b<=this.c)){  
	  return this.b;
           }else return this.c;
     };
     var x=10,y=3,z=5;
     var v = new Vec3(x,y,z);
     var min = v.min();
     var mid = v.mid();
     var max = v.max();
     document.write("min "+min);
     document.write("mid "+mid);
     document.write("max "+max);
						    
						    
    </script>
  </body>
</html>
